				<xs:element minOccurs="0" maxOccurs="1" name="GUI_select_hand" type="GUISelectHand" />
				<xs:element minOccurs="0" maxOccurs="1" name="GUI_select_dynamic_card" type="GUISelectDynamicCard" />
				<xs:element minOccurs="0" maxOccurs="1" name="GUI_select_static_card" type="GUISelectStaticCard" />
