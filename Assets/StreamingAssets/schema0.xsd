			<xs:extension base="Effect">
				<xs:sequence>
					<xs:element minOccurs="0" maxOccurs="1" name="value" type="xs:string" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
