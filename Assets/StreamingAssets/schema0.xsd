	<xs:simpleType name="SpeechType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="normal" />
			<xs:enumeration value="cheat" />
			<xs:enumeration value="threaten" />
			<xs:enumeration value="persuade" />
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="SpeechArt">
		<xs:sequence>
			<xs:element minOccurs="0" maxOccurs="1" name="normal" type="xs:int" />
			<xs:element minOccurs="0" maxOccurs="1" name="cheat" type="xs:int" />
			<xs:element minOccurs="0" maxOccurs="1" name="threaten" type="xs:int" />
			<xs:element minOccurs="0" maxOccurs="1" name="persuade" type="xs:int" />
		</xs:sequence>
	</xs:complexType>
