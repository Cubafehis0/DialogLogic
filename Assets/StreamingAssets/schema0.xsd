			<xs:element minOccurs="0" maxOccurs="1" name="factor" type="xs:string" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ConditionNode" abstract="true" />
	<xs:complexType name="IsCardName">
		<xs:complexContent mixed="false">
			<xs:extension base="ConditionNode">
				<xs:sequence>
					<xs:element minOccurs="0" maxOccurs="1" name="name" type="xs:string" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CountRequirement">
		<xs:complexContent mixed="false">
			<xs:extension base="ConditionNode">
				<xs:sequence>
					<xs:element minOccurs="1" maxOccurs="1" name="compare_type" type="CompareType" />
					<xs:element minOccurs="1" maxOccurs="1" name="value" type="xs:int" />
					<xs:element minOccurs="0" maxOccurs="1" name="requirements" type="ConditionList" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:simpleType name="CompareType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="LT" />
			<xs:enumeration value="GT" />
			<xs:enumeration value="LE" />
			<xs:enumeration value="GE" />
			<xs:enumeration value="EQ" />
			<xs:enumeration value="NE" />
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="ConditionList">
		<xs:sequence>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element minOccurs="0" maxOccurs="1" name="requirement" type="xs:string" />
				<xs:element minOccurs="0" maxOccurs="1" name="all" type="AllNode" />
				<xs:element minOccurs="0" maxOccurs="1" name="any" type="AnyNode" />
				<xs:element minOccurs="0" maxOccurs="1" name="none" type="NoneNode" />
				<xs:element minOccurs="0" maxOccurs="1" name="count_requirement" type="CountRequirement" />
				<xs:element minOccurs="0" maxOccurs="1" name="is_card_name" type="IsCardName" />
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AllNode">
		<xs:complexContent mixed="false">
			<xs:extension base="ConditionList" />
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AnyNode">
		<xs:complexContent mixed="false">
			<xs:extension base="ConditionList" />
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="NoneNode">
		<xs:complexContent mixed="false">
			<xs:extension base="ConditionList" />
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="EffectList">
