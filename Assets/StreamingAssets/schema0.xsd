		<xs:sequence>
			<xs:element minOccurs="0" maxOccurs="1" name="personality_linear" type="Personality" />
			<xs:element minOccurs="0" maxOccurs="unbounded" name="personality_factor" type="xs:float" />
			<xs:element minOccurs="0" maxOccurs="1" name="focus" nillable="true" type="SpeechType" />
			<xs:element minOccurs="0" maxOccurs="1" name="speech" type="SpeechArt" />
			<xs:element minOccurs="0" maxOccurs="1" name="cost_modifier" type="CostModifier" />
			<xs:element minOccurs="0" maxOccurs="1" name="on_turn_start" type="EffectList" />
			<xs:element minOccurs="0" maxOccurs="1" name="on_play_card" type="EffectList" />
			<xs:element minOccurs="0" maxOccurs="1" name="on_buff" type="EffectList" />
