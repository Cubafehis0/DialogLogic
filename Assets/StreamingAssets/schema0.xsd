				<xs:element minOccurs="0" maxOccurs="1" name="discard" type="DiscardCard" />
				<xs:element minOccurs="0" maxOccurs="1" name="execute" type="ExecuteCard" />
				<xs:element minOccurs="0" maxOccurs="1" name="potantial" type="IF" />
				<xs:element minOccurs="0" maxOccurs="1" name="ifelse" type="IFELSE" />
				<xs:element minOccurs="0" maxOccurs="1" name="set_global_variable" type="SetGlobalVar" />
				<xs:element minOccurs="0" maxOccurs="1" name="GUI_select_condition_nerf" type="GUISelectNerfCondition" />
				<xs:element minOccurs="0" maxOccurs="1" name="GUI_select_choice" type="GUISelectCurrentSlot" />
				<xs:element minOccurs="0" maxOccurs="1" name="all_random_reveal" type="AllChoiceRandomReveal" />
				<xs:element minOccurs="0" maxOccurs="1" name="choice_remove_condition" type="ChoiceRemoveCondition" />
				<xs:element minOccurs="0" maxOccurs="1" name="choice_reveal" type="ChoiceReveal" />
