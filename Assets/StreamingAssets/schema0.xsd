			<xs:element minOccurs="0" maxOccurs="1" name="additional_draw" type="xs:int" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Personality">
		<xs:sequence>
			<xs:element minOccurs="0" maxOccurs="1" name="inner" type="xs:int" />
			<xs:element minOccurs="0" maxOccurs="1" name="outside" type="xs:int" />
