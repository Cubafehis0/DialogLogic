				<xs:element minOccurs="0" maxOccurs="1" name="choice_reveal_random" type="ChoiceRevealRandom" />
				<xs:element minOccurs="0" maxOccurs="1" name="current_focus" type="CurrentFocus" />
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
