		<xs:restriction base="xs:string">
			<xs:enumeration value="none" />
			<xs:enumeration value="logic" />
			<xs:enumeration value="spirital" />
			<xs:enumeration value="moral" />
			<xs:enumeration value="immoral" />
			<xs:enumeration value="roundabout" />
			<xs:enumeration value="aggressive" />
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="CardInfo" nillable="true" type="CardInfo" />
	<xs:complexType name="CardInfo">
		<xs:sequence>
			<xs:element minOccurs="1" maxOccurs="1" name="name" nillable="true" type="xs:string" />
			<xs:element minOccurs="1" maxOccurs="1" name="title" nillable="true" type="xs:string" />
			<xs:element minOccurs="1" maxOccurs="1" name="condition_desc" nillable="true" type="xs:string" />
			<xs:element minOccurs="1" maxOccurs="1" name="effect_desc" nillable="true" type="xs:string" />
			<xs:element minOccurs="1" maxOccurs="1" name="meme" nillable="true" type="xs:string" />
